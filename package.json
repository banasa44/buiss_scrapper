{
  "name": "buiss-scrapper",
  "version": "0.1.0",
  "description": "InfoJobs offer scraper with scoring and aggregation",
  "main": "dist/main.js",
  "scripts": {
    "dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "build": "tsc && tsc-alias",
